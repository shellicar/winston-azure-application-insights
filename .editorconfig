# Client side .editorconfig

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
end_of_line = lf
# editorconfig-tools is unable to ignore longs strings or urls
max_line_length = null

[*.ts]
indent_style = space
indent_size = 2

[{package.json,*.yml}]
indent_style = space
indent_size = 2
